{
  "indent": 2,
  "blank_indent": false,
  "profile": "jinja",
  "extension": "html",
  "format_css": false,
  "format_js": false,
  "line_break_after_multiline_tag": true,
  "preserve_blank_lines": false,
  "exclude": [
    "node_modules",
    ".venv",
    "venv"
  ],
  "ignore": "H006,H030,H021",
  "quiet": false,
  "src": "app/templates"
}
